#, python-format
msgid "<b>A file named \"%s\" already exists. Do you want to replace it?</b>"
msgstr "<b>Ka tashmë një kartelë të quajtur “%s”. Doni të zëvendësohet?</b>"

#: ../xfce4-panel-profiles/xfce4-panel-profiles.py:489
#, python-format
msgid ""
"The file already exists in \"%s\", replacing it will overwrite its contents."
msgstr ""
"Kartela ekziston tashmë në “%s”, zëvendësimi i saj do t’i mbishkruajë "
"lëndën."

#: ../xfce4-panel-profiles/xfce4-panel-profiles.py:491
msgid "Replace"
msgstr "Zëvendësoje"
